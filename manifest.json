{
  "manifest_version": 3,
  "name": "VirusTotal Checker",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://www.virustotal.com/*"
  ],
  "icons": {
    "48": "image/virustotal.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "image/virustotal.png"
    }
  }
}
